{
    "name": "starter-tailwindcss",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "predev": "npm run tw:build",
        "dev": "zenith dev",
        "tw:build": "npx @tailwindcss/cli -i ./src/styles/globals.css -o ./src/styles/output.css --minify",
        "tw:watch": "npx @tailwindcss/cli -i ./src/styles/globals.css -o ./src/styles/output.css --watch",
        "prebuild": "npm run tw:build",
        "build": "zenith build",
        "preview": "zenith preview",
        "test": "bun test",
        "lint": "eslint .",
        "format": "prettier --write ."
    },
    "dependencies": {
        "@zenithbuild/core": "0.5.0-beta.2.16"
    },
    "devDependencies": {
        "@tailwindcss/cli": "^4.1.18",
        "@types/bun": "latest",
        "@typescript-eslint/eslint-plugin": "^8.53.0",
        "@typescript-eslint/parser": "^8.53.0",
        "eslint": "^9.39.2",
        "prettier": "^3.7.4",
        "tailwindcss": "^4.1.18"
    }
}